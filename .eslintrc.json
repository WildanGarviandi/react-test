{
  "extends": [
    "airbnb",
    "plugin:flowtype/recommended"
  ],
  "env": {
    "jest": true
  },
  "parser": "babel-eslint",
  "plugins": [
    "react",
    "jsx-a11y",
    "import",
    "flowtype"
  ],
  "globals": {
    "document": true,
    "window": true,
    "localStorage": true,
    "FormData": true,
    "Blob": true
  },
  "parserOptions": {
    "ecmaVersion": 8
  },
  "settings": {
    "flowtype": {
      "onlyFilesWithFlowAnnotation": false
    }
  }
}